@PACKAGE_INIT@

include(GNUInstallDirs)

get_filename_component(_IMPORT_PREFIX "${CMAKE_CURRENT_LIST_FILE}" PATH)
get_filename_component(_IMPORT_PREFIX "${_IMPORT_PREFIX}" PATH)
get_filename_component(_IMPORT_PREFIX "${_IMPORT_PREFIX}" PATH) 
if(_IMPORT_PREFIX STREQUAL "/")
  set(_IMPORT_PREFIX "")
endif()

add_library(wedprcrypto::crypto STATIC IMPORTED)
set_target_properties(wedprcrypto::crypto PROPERTIES
  INTERFACE_INCLUDE_DIRECTORIES "${_IMPORT_PREFIX}/include/"
  IMPORTED_LINK_INTERFACE_LANGUAGES_DEBUG "C;CXX"
  IMPORTED_LOCATION "${_IMPORT_PREFIX}/lib/${CMAKE_STATIC_LIBRARY_PREFIX}ffi_c_crypto_binary${CMAKE_STATIC_LIBRARY_SUFFIX}"
)

add_library(wedprcrypto::bcos-interfaces STATIC IMPORTED)
set_target_properties(wedprcrypto::bcos-interfaces PROPERTIES
  INTERFACE_INCLUDE_DIRECTORIES "${_IMPORT_PREFIX}/include/"
  IMPORTED_LINK_INTERFACE_LANGUAGES_DEBUG "C;CXX"
  IMPORTED_LOCATION "${_IMPORT_PREFIX}/lib/${CMAKE_STATIC_LIBRARY_PREFIX}ffi_c_fisco_bcos${CMAKE_STATIC_LIBRARY_SUFFIX}"
)

add_library(wedprcrypto::zkp STATIC IMPORTED)
set_target_properties(wedprcrypto::zkp PROPERTIES
  INTERFACE_INCLUDE_DIRECTORIES "${_IMPORT_PREFIX}/include/"
  IMPORTED_LINK_INTERFACE_LANGUAGES_DEBUG "C;CXX"
  IMPORTED_LOCATION "${_IMPORT_PREFIX}/lib/${CMAKE_STATIC_LIBRARY_PREFIX}ffi_c_zkp${CMAKE_STATIC_LIBRARY_SUFFIX}"
)